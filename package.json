{
  "name": "SelectR",
  "version": "0.1.1",
  "description": "A Select2 style input built in React",
  "main": "SelectR.jsx",
  "author": "Adam Lind <sunboxnet@gmail.com>",
  "license": "MIT",
  "homepage": "https://bitbucket.org/pharserror/selectr#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/pharserror/selectr.git"
  },
  "keywords": [
    "Select2",
    "React"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "test",
    "tests"
  ],
  "engines": {
    "node": "5.0.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testRegex": "/__tests__/",
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-jest": "^15.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "0.24.0",
    "express": "^4.14.0",
    "jest-cli": "^15.1.1",
    "node-sass": "^3.8.0",
    "path": "0.12.7",
    "react-addons-test-utils": "^15.3.2",
    "react-testutils-additions": "15.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.4",
    "webpack-dev-middleware": "1.6.1"
  },
  "dependencies": {
    "es6-promise": "^3.3.1",
    "history": "^4.2.0",
    "jquery": "^3.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "webpack": "^1.13.2"
  }
}
